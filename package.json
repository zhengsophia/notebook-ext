{
  "name": "notebook-ext",
  "displayName": "notebook-ext",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "meng-notebook",
          "title": "Meng Notebook",
          "icon": "dist/imgs/clover.svg"
        }
      ]
    },
    "views": {
      "meng-notebook": [
        {
          "type": "webview",
          "id": "meng-notebook.treeView",
          "name": "Tree View"
        }
      ]
    },
    "commands": [
      {
        "command": "notebook-ext.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "notebook-ext.getNotebookData",
        "title": "Get Notebook Data"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js && node esbuild.webviews.mjs",
    "watch": "npm-run-all -p watch:*",
    "watch:extension": "node esbuild.js --watch",
    "watch:webviews": "node esbuild.webviews.mjs --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.96.0",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "esbuild": "^0.24.0",
    "eslint": "^9.16.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/material": "^6.4.0",
    "@mui/x-tree-view": "^7.23.6",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  }
}
